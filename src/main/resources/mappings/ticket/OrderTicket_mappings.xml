<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="OrderTicketDAO">
	
	<!-- 티켓구매정보 DB저장 -->
	<insert id="insertOrderTicket">
			INSERT INTO ORDER_TICKET(MEMBER_ID,TICKET_NUM,TICKET_DATE,TICKET_EMAIL,TICKET_PHONE,TICKET_TYPE,TICKET_AMOUNT,TICKET_SUM)
			 VALUES(#{member_id},#{ticket_num},#{ticket_date},#{ticket_email},#{ticket_phone},#{ticket_type},#{ticket_amount},1)
	</insert>

</mapper>
