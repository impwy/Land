<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!--Alias 설정 -->
	<typeAliases>
		<typeAlias  alias="board" type="com.land.myapp.model.board.DTO.BoardDTO"/>
		<typeAlias  alias="SearchCondition" type="com.land.myapp.main.SearchCondition"/>
		    <typeAlias alias="member" type="com.land.myapp.model.member.DTO.MemberDTO" />
		      <typeAlias alias="basket" type="com.land.myapp.model.basket.DTO.BasketDTO" />
		       <typeAlias alias="goods" type="com.land.myapp.model.goods.DTO.GoodsDTO" />
				<typeAlias alias="goodspayment" type="com.land.myapp.model.member.DTO.GoodsPaymentDTO" />
		     
	</typeAliases>
	<!--SQL MAPPER 설정 -->
	<mappers>
		<mapper resource="mappings/board/board-mappings.xml" />
		<mapper resource="mappings/ticket/OrderTicket_mappings.xml"/>
		<mapper resource="mappings/member/member-mappings.xml" />
		<mapper resource="mappings/basket/basket-mappings.xml" />
		<mapper resource="mappings/order_goods/admin-mappings.xml" />
		<mapper resource="mappings/goods/goods_mappings.xml" />
		<mapper resource="mappings/goods/Goods_payment_mappings.xml" />

	</mappers>
</configuration>